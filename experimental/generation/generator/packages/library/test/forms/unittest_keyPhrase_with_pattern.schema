{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "KeyphraseProperty": {
            "type": "string",
            "$entities": [
                "keyPhrase"
            ],
            "pattern" : "^[a-z]{1,3}$"
        }
    },
    "required": [
        "KeyphraseProperty"
    ],
    "$requires": [
        "standard.schema"
    ]
}